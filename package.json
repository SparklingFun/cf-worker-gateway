{
  "name": "@arctome/worker-scaffold",
  "version": "1.0.3",
  "description": "Like scaffold for workers, this framework is designed for developers use Cloudflare Worker to build their Serverless Web Application.",
  "main": "lib/index.js",
  "scripts": {
    "test": "rollup -c ./rollup.config.js && jest",
    "test:coverage": "rollup -c ./rollup.config.js && jest --coverage",
    "test:badges": "npm run test:coverage && make-coverage-badge",
    "build": "tsc --declaration",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "prepare": "husky install",
    "lint": "eslint \"./src/**/*.{js,ts}\" --fix"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "Cloudflare",
    "Cloudflare Worker",
    "Web framework"
  ],
  "author": "Arcto <sparkinglemon@foxmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/arctome/worker-scaffold"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@dollarshaveclub/cloudworker": "^0.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^11.0.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "2.3.1",
    "rollup": "^2.52.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "resolutions": {
    "trim-newlines": "^3.0.1",
    "hosted-git-info": "^2.8.9",
    "minimist": "^0.2.1",
    "handlebars": "^4.7.7"
  },
  "files": [
    "/lib",
    "README.md",
    "package.json"
  ],
  "jest": {
    "coverageReporters": [
      "json-summary"
    ]
  }
}
