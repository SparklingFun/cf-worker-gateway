{
  "name": "cf-worker-gateway",
  "version": "0.4.1-canary.0",
  "description": "A simple gateway for Cloudflare Worker, use before `respondWith`.",
  "main": "lib/index.js",
  "scripts": {
    "test": "rollup -c ./rollup.config.js && jest",
    "test:coverage": "rollup -c ./rollup.config.js && jest --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "build": "tsc --declaration",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "gateway",
    "Worker"
  ],
  "author": "SparklingFun",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/SparklingFun/cf-worker-gateway"
  },
  "dependencies": {
    "glob-to-regexp": "^0.4.1",
    "ipaddr.js": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@cloudflare/kv-asset-handler": "^0.0.12",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@dollarshaveclub/cloudworker": "^0.1.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/glob-to-regexp": "^0.4.0",
    "conventional-changelog-cli": "^2.1.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "rollup": "^2.34.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "files": [
    "/lib",
    "README.md",
    "package.json"
  ],
  "jest": {
    "coverageReporters": [
      "json-summary"
    ]
  }
}
